<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.legalnews.daily</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/apple/legal-daily-news-skill/scripts/fully_auto.sh</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>8</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/apple/legal-daily-news-skill/logs/auto.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/apple/legal-daily-news-skill/logs/auto.error.log</string>

    <key>RunAtLoad</key>
    <false/>

    <key>KeepAlive</key>
    <false/>
</dict>
</plist>
